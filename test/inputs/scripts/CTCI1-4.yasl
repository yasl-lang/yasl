# CTCI 1.4
fn pp(s) {
    let hash = {}
    for c in s {
        if hash[c] !== undef {
            hash[c] += 1
        } else {
            hash[c] = 1
        }
    }

    let count = false
    for c in hash {
        if hash[c] % 2 != 0 {
            if count {
                return false
            }
            count = true
        }
    }

    return true

}


echo pp('asdsa')
echo pp('asdas')
echo pp('sasdasddas')




fn match_list(n) {
    match n {
        [ let x, let y ] {
            return "[ x, y ] are [ #{x}, #{y} ]."
        }
        * {
            return '???'
        }
    }
}

for i <- [ [ 1, 2 ], [ 1 ], [ .a, .b ] ] {
    echo match_list(i)
}


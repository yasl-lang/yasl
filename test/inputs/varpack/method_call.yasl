
const obj = {}

fn obj.f(self, a, b, c) {
    return a, b, c
}

fn obj.g(self, x, y, z) {
    return x + y + z
}

mt.set(obj, obj)

# at the end, expand f's return values
echo obj->g(obj->f(1, 2, 3))

# even when we have params before
echo obj->g(1, obj->f(1, 2, 3))

# if f is not at end, don't expand
echo obj->g(1, obj->f(1, 2, 3), 3)


const obj_mt = {}

fn obj_mt.f() {
    return 1, 2
}

fn obj_mt.g() {
    return .a, .b, .c
}

fn obj_mt.h() {
    return 'first', 'second'
}

const obj = {}
mt.set(obj, obj_mt)

let a, let b, let c = false, obj->f()

echo a
echo b
echo c

a, b, c = true, obj->g()

echo a
echo b
echo c

a, b, c = undef, obj->h()

echo a
echo b
echo c

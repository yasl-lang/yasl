fn compress(s) {
    let new_s = ''
    let i = 0
    while i < len s {
        let count = 0
        let j = i
        while i < len s && s[i] == s[j] {
            count += 1
            i += 1
        }
        new_s ~= s[j] ~ count
    }
    if len s <= len new_s {
        return s
    }
    return new_s
}


echo compress('aabccccaaa')
echo compress('abcde')


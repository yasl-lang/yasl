##fib(0): 0\nfib(1): 1\nfib(2): 1\nfib(3): 2\nfib(4): 3\nfib(5): 5\nfib(6): 8\n


fn fib(n) {
    let i = 0
    let nextterm = 1
    let present = 1
    let previous = 0

    while i < n {
        nextterm = present + previous
        previous = present
        present = nextterm
        i += 1
    }
    return previous
}



fn range(a, b) {
    const tmp = []
    for let i = a; i < b; i += 1 {
        tmp->push(i)
    }

    return tmp
}

for i <- range(0, 7) {
    echo "fib(#{i}): #{fib(i)}"
}

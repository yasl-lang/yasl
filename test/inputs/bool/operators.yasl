
const fn apply(f, op) {
    for i <- [ true, false ] {
        for j <- [ true, false ] {
            echo "#{i} #{op} #{j} => #{f(i, j)}"
        }
    }
}

apply(fn(a, b) -> a | b, '|')
apply(fn(a, b) -> a & b, '&')
apply(fn(a, b) -> a &^ b, '&^')
apply(fn(a, b) -> a ^ b, '^')

echo ^true
echo ^false
